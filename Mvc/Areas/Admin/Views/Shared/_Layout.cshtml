
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>abc</title>
    @Scripts.Render("~/bundles/modernizr")
    <link href="~/Commons/fontawesome-free-6.2.0-web/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/Layout.css" asp-append-version="true" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-iYQeCzEYFbKjA/T2uDLTpkwGzCiq6soy8tYaI1GyVh/UjpbCx/TYkiZhlZB6+fzT" crossorigin="anonymous">
    <link href="~/Content/CssAdmin/css/sb-admin-2.min.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link href="~/Content/CssAdmin/all.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    @RenderSection("css", false)
</head>
<body id="page-top">
    <div id="wrapper">
        @{ Html.RenderAction("LeftMenu", "Layout");}
        <div id="content-wrapper" class="d-flex flex-column">
            <div id="content">
                @{ Html.RenderAction("TopMenu", "Layout");}

                @RenderBody()
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Commons/Admin/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/Commons/Admin/chart.js/Chart.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.1/dist/js/bootstrap.bundle.min.js" integrity="sha384-u1OknCvxWvY5kfmNBILK2hRnQC3Pr17a+RTT6rIHI7NnikvbZlHgTPOOmMi466C8" crossorigin="anonymous"></script>
    <script src="~/Commons/fontawesome-free-6.2.0-web/js/all.min.js"></script>
    <script src="~/Scripts/JsAdmin/js/sb-admin-2.min.js"></script>
    @RenderSection("js", false)
</body>
</html>
